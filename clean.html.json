ition: absolute;\n}\n.hamburger-menu_1680956133137_933 .hamburger-btn span {\n    display: block;\n    width: 30px;\n    height: 5px;\n    margin-bottom: 3px;\n    position: relative;\n    background-color: #000000;\n    border: 1px solid #ffffff;\n    border-radius: 3px;\n    transition: border-color 0.1s ease-in,\n      opacity 0.1s ease-in,\n      transform 0.1s ease-in,\n      background 0.1s ease-in\n}\n\n.hamburger-menu_1680956133137_933.open .hamburger-btn span {\n    border-color: rgba(255, 255, 255, 0);\n}\n\n.hamburger-menu_1680956133137_933 .hamburger-btn span:first-child {\n    transform-origin: 4px -2px;\n}\n.hamburger-menu_1680956133137_933.open .hamburger-btn span:first-child {\n    transform: rotate(45deg) translate(7px, -4px);\n}\n\n.hamburger-menu_1680956133137_933.open .hamburger-btn span:nth-last-child(2) {\n    opacity: 0;\n}\n\n.hamburger-menu_1680956133137_933 .hamburger-btn span:nth-child(3) {\n    transform-origin: -11px 4px;\n}\n.hamburger-menu_1680956133137_933.open .hamburger-btn span:nth-child(3) {\n    transform: rotate(-45deg) translate(0, 12px);\n}\n\n/* themes */\n\n\n</style>\n<!-- inspired by https://codepen.io/erikterwan/pen/EVzeRP -->\n<div class=\"hamburger-menu_1680956133137_933 closed\">\n    <div class=\"hamburger-btn \">\n      <span></span>\n      <span></span>\n      <span></span>\n    </div>\n    <div class=\"nav-container\">\n      <div class=\"prevent-scale\">\n        <nav class=\"\">\n            <ul>\n            \n            </ul>\n        </nav>\n      </div>\n  </div>\n</div>\n<script>\n(function() {\n  var isOpen = false;\n  function close() {\n    isOpen = false;\n    document.querySelector('.hamburger-menu_1680956133137_933').classList.remove('open');\n    document.querySelector('.hamburger-menu_1680956133137_933').classList.add('closed');\n  }\n  function open() {\n    isOpen = true;\n    document.querySelector('.hamburger-menu_1680956133137_933').classList.add('open');\n    document.querySelector('.hamburger